{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adfpyspark1"
		},
		"ls_adlsgen2_historydat_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adlsgen2_historydat'"
		},
		"ls_blob_sales_data_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_blob_sales_data'"
		},
		"ls_azkv_adb_token_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://azkv-sqldb.vault.azure.net/"
		},
		"ls_adlsgen2_historydat_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://pyadlsgen2storage.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adfpyspark1_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_adlsgen2_historydat_accountKey": {
						"value": "[parameters('ls_adlsgen2_historydat_accountKey')]"
					},
					"ls_blob_sales_data_connectionString": {
						"value": "[parameters('ls_blob_sales_data_connectionString')]"
					},
					"ls_azkv_adb_token_properties_typeProperties_baseUrl": {
						"value": "[parameters('ls_azkv_adb_token_properties_typeProperties_baseUrl')]"
					},
					"ls_adlsgen2_historydat_properties_typeProperties_url": {
						"value": "[parameters('ls_adlsgen2_historydat_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}